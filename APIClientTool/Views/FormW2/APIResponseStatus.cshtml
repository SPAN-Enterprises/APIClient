@*@model APIClientTool.ViewModels.ErrorResponse*@
@model APIClientTool.ViewModels.W2ReturnResponse
<div class="modal-header edit-modal-header">
    <button type="button" class="close" data-dismiss="modal"></button>
    <h4 id="form_heading" class="modal-title">
        Reponse Status
    </h4>
</div>
<div class="modal-body edit-modal-body white_bg">
    @if (Model != null)
    {
        <table class="responsive">
            <tr>
                <td class="labelName"><label class="control-label">SubmissionId:</label></td>
                <td class="fieldName">@Model.SubmissionId</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Code:</label></td>
                <td class="fieldName">@Model.StatusCode</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Message:</label></td>
                <td class="fieldName">@Model.StatusMessage</td>
            </tr>
            <tr><td class="spacer10"></td></tr>
            <tr>
                <td class="labelName"><label class="control-label">Status Name:</label></td>
                <td class="fieldName">@Model.StatusName</td>
            </tr>
        </table>
    }
    <div class="spacer15"> </div>
    @if (Model != null && Model.FormW2Record != null && Model.FormW2Record.ErrorRecords != null && Model.FormW2Record.ErrorRecords.Count > 0)
    {
        <h3 style="font-size:medium;font-weight:600">Error Records</h3>
        <div>
            <table class="table table-bordered">
                <thead class="thead-inverse">
                    <tr>
                        <td>
                            Sequence
                        </td>
                        <td>
                            Error
                        </td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.FormW2Record.ErrorRecords)
                {
                        <tr>
                            <td>@item.Sequence</td>
                            <td>
                                <table class="table table-bordered">
                                    <thead class="thead-inverse">
                                        <tr>
                                            <td>Code</td>
                                            <td>Name</td>
                                            <td>Message</td>
                                            <td>Type</td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var itemerror in item.Errors)
                                        {
                                            <tr>
                                                <td>@itemerror.Code</td>
                                                <td>@itemerror.Name</td>
                                                <td>@itemerror.Message</td>
                                                <td>@itemerror.Type</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </td>
                        </tr>

                    }
                </tbody>
            </table>
        </div>
    }
    <div class="spacer15"> </div>

    @if (Model != null && Model.FormW2Record != null && Model.FormW2Record.SuccessRecords != null)
            {
        <h3 style="font-size:medium;font-weight:600">Success Records</h3>
        <div>
            <table class="table table-bordered">
                <thead class="thead-inverse">
                    <tr>
                        <td>Sequence</td>
                        <td>Record Id</td>
                        <td>Record Status</td>
                        <td>Created Ts</td>
                        <td>Updated Ts</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.FormW2Record.SuccessRecords)
                    {
                        <tr>
                            <td>@item.Sequence</td>
                            <td>@item.RecordId</td>
                            <td>@item.RecordStatus</td>
                            <td>@item.CreatedTs</td>
                            <td>@item.UpdatedTs</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    }
</div>

<div class="modal-footer edit-modal-footer mTopNo">

</div>

<script type="text/javascript">
    $(function () {


    })
</script>
